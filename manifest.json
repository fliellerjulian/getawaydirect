{
  "manifest_version": 3,
  "name": "getaway.direct: find direct hosts on AirBNB",
  "version": "1.1",
  "description": "todo",
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://serpapi.com/*"],
  "icons": {},
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.airbnb.com/*",
        "https://www.airbnb.de/*",
        "https://www.airbnb.co.uk/*",
        "https://www.airbnb.ca/*",
        "https://www.airbnb.fr/*",
        "https://www.airbnb.it/*",
        "https://www.airbnb.es/*"
      ],
      "js": ["ExtPay.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.airbnb.com/*",
        "https://www.airbnb.de/*",
        "https://www.airbnb.co.uk/*",
        "https://www.airbnb.ca/*",
        "https://www.airbnb.fr/*",
        "https://www.airbnb.it/*",
        "https://www.airbnb.es/*"
      ],
      "resources": ["assets/add.png"]
    }
  ]
}
